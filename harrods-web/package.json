{
  "name": "harrods-web",
  "private": true,
  "scripts": {
    "dev": "NODE_ENV=local vite",
    "build-dev": "NODE_ENV=development vite build --mode development",
    "build-prod": "NODE_ENV=production vite build --mode production",
    "serve": "vite preview",
    "type-check": "tsc --noEmit",
    "lint": "eslint --fix --ext .js,.jsx,.ts,.tsx src",
    "format": "prettier --write src",
    "test": "jest",
    "upload-dev": "aws s3 sync ./dist s3://musinsa-data-harrods-web/dev",
    "upload-prod": "aws s3 sync ./dist s3://musinsa-data-harrods-web/prod",
    "invalidate-dev": "aws cloudfront create-invalidation --distribution-id EH3LD2XMQ5CTO --paths \"/*\"",
    "invalidate-prod": "aws cloudfront create-invalidation --distribution-id E1DUIMXZZLKNBG --paths \"/*\"",
    "deploy-dev": "npm run build-dev && npm run upload-dev && npm run invalidate-dev",
    "deploy-prod": "npm run build-prod && npm run upload-prod && npm run invalidate-prod"
  },
  "dependencies": {
    "cron-validator": "^1.3.1",
    "dotenv": "^16.3.1",
    "email-validator": "^2.0.4",
    "ra-data-json-server": "^4.14.0",
    "react": "^18.2.0",
    "react-admin": "^4.14.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.1.4",
    "@testing-library/react": "^14.1.0",
    "@types/jest": "^29.5.8",
    "@types/node": "^18.16.1",
    "@types/react": "^18.0.22",
    "@types/react-dom": "^18.0.7",
    "@typescript-eslint/eslint-plugin": "^5.60.1",
    "@typescript-eslint/parser": "^5.60.1",
    "@vitejs/plugin-react": "^4.0.1",
    "eslint": "^8.43.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jest-svg-transformer": "^1.0.0",
    "prettier": "^2.8.8",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.6",
    "vite": "^4.3.9"
  }
}
